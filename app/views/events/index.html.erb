    <tr>
      <th>Name &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp</th>
      <th>Greatest Performance</th>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @events.each do |event| %>
    <tr>
          <td><%= event.name %></td>
          <td><%= event.bestperformance %>  &nbsp &nbsp </td>
          <td><button><a href="/events/<%= event.id %>">Show</a></button>&nbsp</td>
