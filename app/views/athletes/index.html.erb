<h1>Athletes</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp</th>
      <th>Age &nbsp &nbsp</th>
      <th>Country</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @athletes.each do |athlete| %>
    <tr>
          <td><%= athlete.name %></td>
          <td><%= athlete.age %></td>
          <td><%= athlete.country %> &nbsp &nbsp</td>
          <td><button><a href="/athletes/<%= athlete.id %>">Show</a></button>&nbsp</td>
          <td><button><a href="/athletes/<%= athlete.id %>/edit">Edit</a></button>&nbsp</td>
          <td><button><a href="/athletes/<%= athlete.id %>/destroy">Destroy</a></button></td>
    </tr>
  <% end %>
  </tbody>
</table>

